worker_processes auto;
pid {{ nginx_pid }};

events {
	worker_connections 768;
}

http {
    server {
        listen {{ nginx_listen }} default_server;

        root {{ web_root }};
        index index.html index.htm;
        server_name {{ hostname }};

        location = / {
          return 302 {{ web_redirect }};
        }

        location / {
          try_files $uri $uri/ =404;
        }
    }
}
